extends layout

block content
    form(action = action, method = "POST")
        div
            label(for = 'firstSide') first side
            input(name = 'firstSide' value = card.firstSide)
        div
            label(for = 'secondSide') second side
            input(name = 'secondSide' value = card.secondSide)
        div
            button(type = 'submit') send